    <header/>
    <body>
      <!-- GENERAL-->
      <trans-unit id="section.header" resname="section.header">
        <source>%s | CType Manager</source>
        <target state="needs-translation">%s | CType Manager</target>
      </trans-unit>
      <trans-unit id="message.header.2" resname="message.header.2">
        <source>Oops</source>
        <target state="needs-translation">Oops</target>
      </trans-unit>
      <trans-unit id="message.header.0" resname="message.header.0">
        <source>OK</source>
        <target state="needs-translation">OK</target>
      </trans-unit>
      <!-- FALLBACK -->
      <trans-unit id="group.unassigned" resname="group.unassigned" approved="yes">
        <source>Unassigned</source>
        <target state="final">Nicht zugewiesen</target>
      </trans-unit>
      <!-- SECTIONS -->
      <trans-unit id="section.index" resname="section.index">
        <source>Page "%s"</source>
        <target state="needs-translation">Page "%s"</target>
      </trans-unit>
      <trans-unit id="section.cleanup" resname="section.cleanup">
        <source>Cleanup</source>
        <target state="needs-translation">Cleanup</target>
      </trans-unit>
      <trans-unit id="section.overview" resname="section.cleanup">
        <source>Overview</source>
        <target state="needs-translation">Overview</target>
      </trans-unit>
      <!-- INDEX -->
      <trans-unit id="mlang_tabs_tab" resname="mlang_tabs_tab" approved="yes">
        <source>CType Manager</source>
        <target state="final">CType-Manager</target>
      </trans-unit>
      <trans-unit id="mlang_labels_tabdescr" resname="mlang_labels_tabdescr" approved="yes">
        <source>CType Manager</source>
        <target state="final">CType-Manager</target>
      </trans-unit>
      <trans-unit id="index.bodytext" resname="index.bodytext" approved="yes">
        <source>Choose all content types, which shall be available on this page and it's children.</source>
        <target state="final">Wählen Sie alle Inhaltstypen, die auf dieser Seite und ihren Unterseiten verfügbar sein sollen.</target>
      </trans-unit>
      <trans-unit id="index.nopage" resname="index.nopage" approved="yes">
        <source>Please select a page from the page tree.</source>
        <target state="final">Bitte wählen Sie eine Seite aus dem Seitenbaum.</target>
      </trans-unit>
      <trans-unit id="index.button.submit" resname="index.button.submit" approved="yes">
        <source>Save</source>
        <target state="final">Speichern</target>
      </trans-unit>
      <trans-unit id="index.message.bodytext" resname="index.message.bodytext">
        <source>CType Manager configuration has been saved.</source>
        <target state="needs-translation">CType Manager configuration has been saved.</target>
      </trans-unit>
      <trans-unit id="index.switch.label" resname="index.switch.label" approved="yes">
        <source>Enable all</source>
        <target state="final">Alle aktivieren</target>
      </trans-unit>
      <trans-unit id="index.switch.all" resname="index.switch.all" approved="yes">
        <source>Enable all content element types</source>
        <target state="final">Alle Inhaltselementtypen aktivieren</target>
      </trans-unit>
      <trans-unit id="index.switch.group" resname="index.toggle.group" approved="yes">
        <source>Enable all content element types in group "%s"</source>
        <target state="final">Alle Inhaltselementtypen in Gruppe "%s" aktivieren</target>
      </trans-unit>
      <trans-unit id="index.listTypes.headline" resname="index.listTypes.headline" approved="yes">
        <source>Available plugins</source>
        <target state="final">Verfügbare Plugins</target>
      </trans-unit>
      <!-- CLEANUP -->
      <trans-unit id="cleanup.bodytext" resname="cleanup.bodytext" approved="yes">
        <source>Remove the configurations made by the CType Manager.</source>
        <target state="final">Die vom CType-Manager vorgenommenen Konfigurationen entfernen.</target>
      </trans-unit>
      <trans-unit id="cleanup.mode.label" resname="cleanup.mode.label" approved="yes">
        <source>Cleanup mode</source>
        <target state="final">Bereinigungsmodus</target>
      </trans-unit>
      <trans-unit id="cleanup.mode.page" resname="cleanup.mode.page" approved="yes">
        <source>Current page: %s [%s]</source>
        <target state="final">Aktuelle Seite: %s [%s]</target>
      </trans-unit>
      <trans-unit id="cleanup.mode.rootline" resname="cleanup.mode.rootline">
        <source>Rootline</source>
        <target state="needs-translation">Rootline</target>
      </trans-unit>
      <trans-unit id="cleanup.mode.rootpage" resname="cleanup.mode.rootpage">
        <source>Rootpage</source>
        <target state="needs-translation">Rootpage</target>
      </trans-unit>
      <trans-unit id="cleanup.mode.all" resname="cleanup.mode.all" approved="yes">
        <source>All (this affects ALL pages and might take some time!)</source>
        <target state="final">Alles (dies betrifft ALLE Seiten und kann einige Zeit dauern!)</target>
      </trans-unit>
      <trans-unit id="cleanup.submit" resname="cleanup.approval.submit" approved="yes">
        <source>Clean up</source>
        <target state="final">Bereinigen</target>
      </trans-unit>
      <trans-unit id="cleanup.message.error.pageuid.bodytext" resname="cleanup.message.error.pageuid.bodytext">
        <source>A page uid is missing. Please select a page in the page tree and try again.</source>
        <target state="needs-translation">A page uid is missing. Please select a page in the page tree and try again.</target>
      </trans-unit>
      <trans-unit id="cleanup.message.error.cleanupMode.bodytext" resname="cleanup.message.error.cleanupMode.bodytext">
        <source>A cleanup mode is missing. Please select a mode to clean up.</source>
        <target state="needs-translation">A cleanup mode is missing. Please select a mode to clean up.</target>
      </trans-unit>
      <!-- APPROVAL -->
      <trans-unit id="cleanup.approval.headline" resname="cleanup.approval.headline" approved="yes">
        <source>Are you sure?</source>
        <target state="final">Sind Sie sicher?</target>
      </trans-unit>
      <trans-unit id="cleanup.approval.bodytext" resname="cleanup.approval.bodytext" approved="yes">
        <source>You are going to remove the configurations made by the CType Manager.</source>
        <target state="final">Sie werden die vom CType Manager vorgenommenen Konfigurationen entfernen.</target>
      </trans-unit>
      <trans-unit id="cleanup.approval.submit" resname="cleanup.approval.submit" approved="yes">
        <source>Yes, clean up.</source>
        <target state="final">Ja, bereinigen.</target>
      </trans-unit>
      <trans-unit id="cleanup.approval.abort" resname="cleanup.approval.abort" approved="yes">
        <source>No, abort!</source>
        <target state="final">Nein, abbrechen!</target>
      </trans-unit>
      <trans-unit id="cleanup.message.bodytext" resname="cleanup.message.bodytext">
        <source>CType Manager configuration has been removed.</source>
        <target state="needs-translation">CType Manager configuration has been removed.</target>
      </trans-unit>
      <!-- OVERVIEW -->
      <trans-unit id="overview.bodytext" resname="overview.bodytext">
        <source>The following pages have been configured with the CType Manager.</source>
        <target state="needs-translation">The following pages have been configured with the CType Manager.</target>
      </trans-unit>
      <trans-unit id="overview.bodytext.noPages" resname="overview.bodytext.noPages">
        <source>There are currently no pages configured with the CType Manager.</source>
        <target state="needs-translation">There are currently no pages configured with the CType Manager.</target>
      </trans-unit>
      <trans-unit id="overview.table.head.page" resname="overview.table.head.page" approved="yes">
        <source>Page</source>
        <target state="final">Seite</target>
      </trans-unit>
      <trans-unit id="overview.table.head.ctypes" resname="overview.table.head.ctypes" approved="yes">
        <source>Allowed CTypes</source>
        <target state="final">Erlaubte Inhaltselemente</target>
      </trans-unit>
      <trans-unit id="overview.table.head.listTypes" resname="overview.table.head.listTypes">
        <source>Allowed plugins</source>
        <target state="needs-translation">Allowed plugins</target>
      </trans-unit>
      <trans-unit id="overview.noRestrictions" resname="overview.noRestrictions" approved="yes">
        <source>No restrictions</source>
        <target state="final">Keine Beschränkungen</target>
      </trans-unit>
      <trans-unit id="overview.toolbar.edit.label" resname="overview.toolbar.edit.label" approved="yes">
        <source>Edit CType configuration of page "%s".</source>
        <target state="final">CType-Konfiguration der Seite "%s" bearbeiten.</target>
      </trans-unit>
      <trans-unit id="overview.toolbar.delete.label" resname="overview.toolbar.delete.label" approved="yes">
        <source>Delete CType configuration of page "%s".</source>
        <target state="final">CType-Konfiguration der Seite "%s" löschen.</target>
      </trans-unit>
    </body>
