<?xml version="1.0" encoding="UTF-8"?>
<xliff version="1.0" xmlns="urn:oasis:names:tc:xliff:document:1.1">
   <file source-language="en" datatype="plaintext" original="EXT:ctype_manager/Resources/Private/Language/locallang_mod.xlf" date="2022-02-27T23:55:00Z" product-name="ctype-manager">
      <header/>
      <body>
          <!-- GENERAL-->
          <trans-unit id="section.header" resname="section.header">
              <source>%s | CType Manager</source>
          </trans-unit>
          <trans-unit id="message.header.2" resname="message.header.2">
              <source>Oops</source>
          </trans-unit>
          <trans-unit id="message.header.0" resname="message.header.0">
               <source>OK</source>
          </trans-unit>
          
          <!-- FALLBACK -->
          <trans-unit id="group.unassigned" resname="group.unassigned">
               <source>Unassigned</source>
          </trans-unit>
          
          <!-- SECTIONS -->
          <trans-unit id="section.index" resname="section.index">
               <source>Page "%s"</source>
          </trans-unit>
          <trans-unit id="section.cleanup" resname="section.cleanup">
               <source>Cleanup</source>
          </trans-unit>
          <trans-unit id="section.overview" resname="section.cleanup">
               <source>Overview</source>
          </trans-unit>
          
          <!-- INDEX -->
          <trans-unit id="mlang_tabs_tab" resname="mlang_tabs_tab">
               <source>CType Manager</source>
          </trans-unit>
          <trans-unit id="mlang_labels_tabdescr" resname="mlang_labels_tabdescr">
               <source>CType Manager</source>
          </trans-unit>
          <trans-unit id="index.bodytext" resname="index.bodytext">
               <source>Choose all content types, which shall be available on this page and it's children.</source>
          </trans-unit>
          <trans-unit id="index.nopage" resname="index.nopage">
               <source>Please select a page from the page tree.</source>
          </trans-unit>
          <trans-unit id="index.button.submit" resname="index.button.submit">
               <source>Save</source>
          </trans-unit>
          <trans-unit id="index.message.bodytext" resname="index.message.bodytext">
               <source>CType Manager configuration has been saved.</source>
          </trans-unit>
          <trans-unit id="index.switch.label" resname="index.switch.label">
               <source>Enable all</source>
          </trans-unit>
          <trans-unit id="index.switch.all" resname="index.switch.all">
               <source>Enable all content element types</source>
          </trans-unit>
          <trans-unit id="index.switch.group" resname="index.toggle.group">
               <source>Enable all content element types in group "%s"</source>
          </trans-unit>
          <trans-unit id="index.listTypes.headline" resname="index.listTypes.headline">
               <source>Available plugins</source>
          </trans-unit>

          <!-- CLEANUP -->
          <trans-unit id="cleanup.bodytext" resname="cleanup.bodytext">
               <source>Remove the configurations made by the CType Manager.</source>
          </trans-unit>
          <trans-unit id="cleanup.mode.label" resname="cleanup.mode.label">
              <source>Cleanup mode</source>
          </trans-unit>
          <trans-unit id="cleanup.mode.page" resname="cleanup.mode.page">
              <source>Current page: %s [%s]</source>
          </trans-unit>
          <trans-unit id="cleanup.mode.rootline" resname="cleanup.mode.rootline">
              <source>Rootline</source>
          </trans-unit>
          <trans-unit id="cleanup.mode.rootpage" resname="cleanup.mode.rootpage">
              <source>Rootpage</source>
          </trans-unit>
          <trans-unit id="cleanup.mode.all" resname="cleanup.mode.all">
              <source>All (this affects ALL pages and might take some time!)</source>
          </trans-unit>
          <trans-unit id="cleanup.submit" resname="cleanup.approval.submit">
              <source>Clean up</source>
          </trans-unit>
          <trans-unit id="cleanup.message.error.pageuid.bodytext" resname="cleanup.message.error.pageuid.bodytext">
              <source>A page uid is missing. Please select a page in the page tree and try again.</source>
          </trans-unit>
          <trans-unit id="cleanup.message.error.cleanupMode.bodytext" resname="cleanup.message.error.cleanupMode.bodytext">
              <source>A cleanup mode is missing. Please select a mode to clean up.</source>
          </trans-unit>

          <!-- APPROVAL -->
          <trans-unit id="cleanup.approval.headline" resname="cleanup.approval.headline">
              <source>Are you sure?</source>
          </trans-unit>
          <trans-unit id="cleanup.approval.bodytext" resname="cleanup.approval.bodytext">
              <source>You are going to remove the configurations made by the CType Manager.</source>
          </trans-unit>
          <trans-unit id="cleanup.approval.submit" resname="cleanup.approval.submit">
              <source>Yes, clean up.</source>
          </trans-unit>
          <trans-unit id="cleanup.approval.abort" resname="cleanup.approval.abort">
              <source>No, abort!</source>
          </trans-unit>
          <trans-unit id="cleanup.message.bodytext" resname="cleanup.message.bodytext">
               <source>CType Manager configuration has been removed.</source>
          </trans-unit>

          <!-- OVERVIEW -->
          <trans-unit id="overview.bodytext" resname="overview.bodytext">
              <source>The following pages have been configured with the CType Manager.</source>
          </trans-unit>
          <trans-unit id="overview.bodytext.noPages" resname="overview.bodytext.noPages">
              <source>There are currently no pages configured with the CType Manager.</source>
          </trans-unit>
          <trans-unit id="overview.table.head.page" resname="overview.table.head.page">
              <source>Page</source>
          </trans-unit>
          <trans-unit id="overview.table.head.ctypes" resname="overview.table.head.ctypes">
              <source>Allowed CTypes</source>
          </trans-unit>
          <trans-unit id="overview.table.head.listTypes" resname="overview.table.head.listTypes">
              <source>Allowed plugins</source>
          </trans-unit>
          <trans-unit id="overview.none" resname="overview.none">
              <source>None</source>
          </trans-unit>
          <trans-unit id="overview.toolbar.edit.label" resname="overview.toolbar.edit.label">
              <source>Edit CType configuration of page "%s".</source>
          </trans-unit>
          <trans-unit id="overview.toolbar.delete.label" resname="overview.toolbar.delete.label">
              <source>Delete CType configuration of page "%s".</source>
          </trans-unit>
      </body>
   </file>
