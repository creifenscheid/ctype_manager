{
	"name" : "creifenscheid/ctype-manager",
	"type" : "typo3-cms-extension",
	"description" : "This TYPO3 extension implements a backend module through which available content and plugin types can be configured on page level.",
	"license" : "GPL-2.0-or-later",
	"homepage" : "https://github.com/creifenscheid/ctype_manager",
	"require" : {
		"php": "^8.0||^8.1",
		"typo3/cms-core" : "^11.5"
	},
	"require-dev": {
		"roave/security-advisories": "dev-latest"
	},
	"autoload" : {
		"psr-4" : {
			"CReifenscheid\\CtypeManager\\" : "Classes/"
		}
	},
    "replace": {
        "typo3-ter/ctype-manager": "self.version"
    },
	"minimum-stability" : "stable",
	"authors" : [{
			"name" : "Christian Reifenscheid",
			"email" : "dev@christian-reifenscheid.de"
		}
	],
    "extra": {
        "typo3/cms": {
            "extension-key": "ctype_manager"
        }
    }
}
